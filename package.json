{
	"name": "sbotics-extension",
	"displayName": "sBotics",
	"description": "Support and Snippets to rEduc and C#.",
	"version": "1.0.2",
	"publisher": "sbotics-simulator",
	"repository": {
		"type": "git",
		"url": "https://github.com/gabrieldp23/sBotics_Snippets_vscode"
	},
	"license": "GNU License",
	"icon": "assets/logo.png",
	"engines": {
		"vscode": "^1.46.0"
	},
	"galleryBanner": {
		"color": "#36393F",
		"theme": "dark"
	},
	"categories": [
		"Snippets",
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"snippets",
		"snippet",
		"support",
		"languages",
		"C#",
		"csharp",
		"reduc",
		"rEduc",
		"re",
		"sbotics",
		"sBotics"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension",
	"contributes": {
		"snippets": [
			{
				"language": "reduc-en",
				"path": "./snippets/reduc_en.code-snippets"
			},
			{
				"language": "reduc-ptbr",
				"path": "./snippets/reduc_ptbr.code-snippets"
			},
			{
				"language": "csharp-en",
				"path": "./snippets/csharp_en.code-snippets"
			},
			{
				"language": "csharp-ptbr",
				"path": "./snippets/csharp_ptbr.code-snippets"
			},
			{
				"language": "csharp",
				"path": "./snippets/csharp_en.code-snippets"
			}
		],
		"languages": [
			{
				"id": "reduc-en",
				"aliases": [
					"reducEN"
				],
				"extensions": [
					".re",
					".sboticsR"
				],
				"configuration": "./support/language-configuration.json"
			},
			{
				"id": "reduc-ptbr",
				"aliases": [
					"reducPTBR"
				],
				"extensions": [
					".re",
					".sboticsR"
				],
				"configuration": "./support/language-configuration.json"
			},
			{
				"id": "csharp-en",
				"aliases": [
					"csharpEN"
				],
				"extensions": [
					".cs",
					"sboticsC"
				],
				"configuration": "./support/language-configuration.json"
			},
			{
				"id": "csharp-ptbr",
				"aliases": [
					"csharpPTBR"
				],
				"extensions": [
					".cs",
					"sboticsC"
				],
				"configuration": "./support/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "reduc-en",
				"scopeName": "source.sboticsR",
				"path": "./support/grammars/sboticsR.tmLanguage.json"
			},
			{
				"language": "reduc-ptbr",
				"scopeName": "source.sboticsR",
				"path": "./support/grammars/sboticsR.tmLanguage.json"
			},
			{
				"language": "csharp-en",
				"scopeName": "source.sboticsC",
				"path": "./support/grammars/sboticsC.tmLanguage.json"
			},
			{
				"language": "csharp-ptbr",
				"scopeName": "source.sboticsC",
				"path": "./support/grammars/sboticsC.tmLanguage.json"
			},
			{
				"language": "csharp",
				"scopeName": "source.sboticsC",
				"path": "./support/grammars/sboticsC.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.20.7",
		"@types/vscode": "^1.38.0",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"fs": "0.0.1-security"
	}
}